{
  "name": "locust-ui",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "./lib/webui.js",
  "types": "./lib/types/lib.d.ts",
  "files": [
    "lib",
    "README.md",
    "../../LICENSE"
  ],
  "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies; delete this.scripts\"",
  "scripts": {
    "dev": "vite",
    "dev:watch": "vite build --watch",
    "build:ui": "vite build",
    "build:lib": "vite build --config vite.lib.config.ts",
    "build:report": "vite build --config vite.report.config.ts",
    "build": "yarn clean && npm-run-all --parallel build:ui build:report build:lib",
    "clean": "rimraf dist",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "format": "prettier --write '**/**/*.{ts,tsx}'",
    "type-check": "tsc",
    "test": "vitest --silent=false"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5"
  },
  "peerDependenciesMeta": {
    "react-redux": {
      "optional": true
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.9",
    "@mui/material": "^5.14.10",
    "@reduxjs/toolkit": "^2.2.6",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "echarts": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.12",
    "react-markdown": "^9.0.0",
    "react-redux": "^9.1.2",
    "vite": "^4.5.1",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-singlefile": "^2.0.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jsdom": "^22.1.0",
    "msw": "^2.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "vitest-webgl-canvas-mock": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
